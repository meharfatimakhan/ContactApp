<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle"
    android:dither="true">
    <gradient
        android:angle="45"
        android:endColor="#09336D"
        android:startColor="#13365A"
        android:type="linear"
        />

    <stroke
        android:drawable="@android:color/transparent"
        android:width="1dp"
        android:color="#09336D" >
    </stroke>

    <solid android:color="#09336D" />
    <corners
        android:radius="25dp"/>
    <padding
        android:bottom="1dp"
        android:right="1dp"
        android:left="1dp"
        android:top="1dp" />

</shape>

